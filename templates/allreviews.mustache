        <section class="program-allacctivites-detail">
            <!-- All Activities Section-->
                <div class="d-flex justify-content-between align-items-center w-100 count-download-box">
                    <span class="d-flex justify-content-start align-items-center"> 
                        <span class="d-inline-block lightgrey-text font-13">Showing result of</span>
                        <span class="d-inline-block font-12 black-light-color font-w-600" style="margin-left: 3.5px;">{{to_reviews}}</span>
                    </span>
                    <a class="accent-blue font-13" style="font-weight: 600;" id="download" href="{{&downloadurl}}"> <img src="pix/download-bold.svg" alt="" class="mr-2">Download CSV </a>
                </div>
                <ul class="all-to-review-ul" id="">
                    <li class="all-to-review-header">
                        <span class="d-inline-block activity">Activity</span>
                        <div class="submiss-review">
                            <span class="d-inline-block submiss">Submissions </span>
                            <span class="d-inline-block pending">Pending review</span>
                        </div>

                        <span class="d-inline-block action ">Action</span>
                    </li>
                    {{#activity}}
                        <li class=" all-to-review-list">
                            <div class="d-inline-block activity">
                                <div class="font-12 black-light-color">
                                    <span class="d-inline-block">{{&program_name}} </span>
                                    <i class="fa fa-angle-right ml-1 mr-1 d-inline-block" style="color: #484848 !important;"></i>
                                    <span class="d-inline-block"> {{&course_name}}</span>
                                </div>
                                <div class="font-14 solid-blck">
                                    <span class="font-w-600 font-13 d-block">{{&name}}</span>
                                </div>
                            </div>

                            <div class="submiss-review">
                                <div class="d-inline-block submiss">
                                    <span class="desktop-none mb-2  mobile-block font-14 black-light-color"> Submissions </span>
                                    <span class="d-block font-13">{{submit}}</span>
                                </div>
                                <div class="d-inline-block pending">
                                    <span class="desktop-none mb-2  mobile-block font-14 black-light-color"> Pending review</span>
                                    <span class="d-block font-13">{{review}}</span>
                                </div>
                            </div>
                            <div class="d-inline-block action">
                                <!--<span class="desktop-none mb-2  mobile-block font-14 black-light-color">Action</span>-->
                                <a href="{{&gradeurl}}" class="d-flex align-items-center w-100 ">
                                    <span class="d-block font-12 text-right accent-blue font-w-600">Grade now 

                                    </span>
                                    <i class="fa fa-angle-right ml-1 d-inline-block font-13 font-weignt-bolder" style="color: #006fcb !important;"></i>
                                </a>
                            </div>
                        </li>
                    {{/activity}}
                </ul>
        </section>